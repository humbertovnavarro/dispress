generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model Permissions {
  permission String @unique
  role String
}

model ChannelRestrictions {
  channel String @unique
  regex String
  error String
}

model MusicPlays {
  id String @id @default(cuid())
  user String
  url String
  date DateTime @default(now())
}

model SongLikes {
  id String @id @default(cuid())
  user String
  url String
  date DateTime @default(now())
}

model SongDislikes {
  id String @id @default(cuid())
  user String
  url String
  date DateTime @default(now())
}

model User {
  id String @unique
  spotifyPlaylist String?
}